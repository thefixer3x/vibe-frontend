# Testing Environment Variables
# Copy to .env.local and fill in your test credentials

# Neon Database (for MCP bridges)
NEON_API_KEY=your_test_neon_api_key
NEON_CONNECTION_STRING=postgresql://user:password@host:5432/dbname?sslmode=require

# Or use DATABASE_URL as fallback
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Other required vars (from your existing .env.example)
POSTGRES_URL=postgresql://user:pass@host:5432/db
AUTH_SECRET=your-auth-secret
SINGLE_USER_MODE=true
ADMIN_ACCESS_KEY=set-a-strong-random-key
ADMIN_EMAIL=info@lanonasis.com

# Memory Service
NEXT_PUBLIC_MEMORY_API_URL=/api/memory
NEXT_PUBLIC_MEMORY_API_KEY=your-memory-api-key

# Gateway
NEXT_PUBLIC_GATEWAY_URL=http://localhost:3001

# MCP
NEXT_PUBLIC_MCP_SERVER_URL=ws://localhost:3002/mcp
NEXT_PUBLIC_MCP_MODE=auto
NEXT_PUBLIC_ENABLE_MCP=true

# Environment
NODE_ENV=development
BASE_URL=http://localhost:3000
